    <children>
        <Button fx:id="folderBtn" layoutX="41.0" layoutY="33.0" onAction="#selectFolderHandler" text="Select Folder" />
        <Label fx:id="folderPathLbl" layoutX="148.0" layoutY="37.0" minHeight="16" minWidth="69" />
        <Label layoutX="135.0" layoutY="146.0" text="Paired IP Addresses:" />
        <Button fx:id="rereshBtn" layoutX="45.0" layoutY="167.0" mnemonicParsing="false" onAction="#refreshHandler" text="Refresh List" />
        <ListView fx:id="ipAddrListView" layoutX="135.0" layoutY="167.0" prefHeight="100.0" prefWidth="200.0" />
        <Button fx:id="brdcstBtn" layoutX="43.0" layoutY="88.0" mnemonicParsing="false" onAction="#broadcastHandler" text="Broadcast IP" />
    </children>
</AnchorPane>
